{
  "breadcrumbs.enabled": false,
  "editor.fontFamily": "Hack, monospace",
  "editor.fontSize": 12,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.scrollBeyondLastLine": false,
  "editor.tabSize": 2,
  "eslint.alwaysShowStatus": true,
  "eslint.validate": ["javascript", "javascriptreact", "typescript"],
  "explorer.openEditors.visible": 0,
  "git.path": "__GIT_EXECUTABLE_PATH__",
  "javascript.suggest.autoImports": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown-preview-enhanced.previewTheme": "github-dark.css",
  "regex-previewer.enableCodeLens": false,
  "shellcheck.disableVersionCheck": true,
  "shellcheck.exclude": ["SC1090", "SC1091"],
  "shellcheck.executablePath": "__SHELLCHECK_EXECUTABLE_PATH__",
  "shellcheck.run": "onSave",
  "typescript.suggest.autoImports": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "vim.debug.silent": true,
  "vim.easymotion": true,
  "vim.easymotionMarkerFontFamily": "Hack",
  "vim.highlightedyank.enable": true,
  "vim.leader": ",",
  "vim.mouseSelectionGoesIntoVisualMode": false,
  "vim.sneak": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.useSystemClipboard": true,
  "vsicons.dontShowNewVersionMessage": true,
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Nord",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.iconTheme": "seti"
}
